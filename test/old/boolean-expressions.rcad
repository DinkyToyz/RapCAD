module println(msg)
{
echo(msg,"\n");
}

//Not
println("===Not===");
println(!false);
println(!true);

//Negate
println("===Negate===");
println(-false);
println(-true);

//Unary Plus
println("===Unary Plus===");
println(+false);
println(+true);

//Add
println("===Add===");
println(false+false);
println(false+true);
println(true+false);
println(true+true);

//Subtract
println("===Subtract===");
println(false-false);
println(false-true);
println(true-false);
println(true-true);

//And
println("===And===");
println(false&&false);
println(false&&true);
println(true&&false);
println(true&&true);

//Or
println("===Or===");
println(false||false);
println(false||true);
println(true||false);
println(true||true);

//Exponent
println("===Exponent===");
println(false^false);
println(false^true);
println(true^false);
println(true^true);

//Multiply
println("===Multiply===");
println(false*false);
println(false*true);
println(true*false);
println(true*true);

//Divide
println("===Divide===");
//println(false/false); divide by zero
println(false/true);
//println(true/false); divide by zero
println(true/true);
